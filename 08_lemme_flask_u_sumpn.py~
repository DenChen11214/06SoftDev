from flask import Flask

app = Flask(__name__)

@app.route("/")
def hello_world():
    print("about to print __name__...")
    print(__name__)
    return "No hablo queso!"

@app.route("/morestuff")
def more_stuff():
    return "This page has MORE STUFF"


if __name__ == "__main__":
    app.debug = True
    app.run()

